{
  "format_version": "1.16.0",
  "minecraft:entity": {
    "description": {
      "identifier": "disenchanter:despawn",
      "is_spawnable": false,
      "is_summonable": true,
      "is_experimental": false
    },
    "component_groups": {
			
		"despawn":{
			"minecraft:instant_despawn":{}
		}
    },

    "components": {
	"minecraft:timer": {
			"looping": false,
			"time": [0,0],
			"randomInterval": false,
			"time_down_event": {
				"event": "despawn",
				"target": "self"
			}
			},
	"minecraft:persistent":{},
      "minecraft:type_family": {
        "family": [ "disenchant" ]
      },
      
      "minecraft:collision_box": {
        "width": 0.5,
        "height": 0.9
      },
      "minecraft:damage_sensor": {
        "triggers": {
          "cause": "all",
          "deals_damage": false
        }
      },
	  "minecraft:knockback_resistance": {
        "value": 1
      },
      "minecraft:hurt_on_condition": {
        "damage_conditions": [
          {
            "filters": { "test": "in_lava", "subject": "self", "operator": "==", "value": true },
            "cause": "lava",
            "damage_per_tick": 4
          }
        ]
      },
      "minecraft:health": {
        "value": 1,
        "max": 1
      },
      "minecraft:nameable": {
      },
      "minecraft:physics": {
	  "has_gravity": false,
	  "has_collision": false
      },
	"minecraft:nameable":{
	"allow_name_tag_renaming": false	
	},
      "minecraft:conditional_bandwidth_optimization": {
      }
    },

    "events": {
		"reset":{
			"remove":{"component_groups":["disenchant","disenchant_timer"]}
		},
		"disenchant":{
			"add":{"component_groups":["disenchant"]}
		},
		"disenchant_timer":{
			"add":{"component_groups":["disenchant_timer"]},
			"run_command":{
				"command": "say ahh"
			}
		},
		"research_done":{
			"remove":{"component_groups":["reseach_timer"]}
		},
		"despawn":{
			
			"add":{"component_groups":["despawn"]}
		}
    }
  }
}
